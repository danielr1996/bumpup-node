{
  "name": "@bumpup/mono",
  "version": "1.0.0",
  "private": true,
  "description": "this package.json is just used to track dependencies of the individual packages of this monorepo",
  "scripts": {
    "watch-core-cli": "cd core/cli && npm run watch",
    "watch-core-fp": "cd core/fp && npm run watch",
    "watch-core-lib": "cd core/lib && npm run watch",
    "watch-plugins-version-package-json": "cd plugins/version-package-json && npm run watch",
    "watch-plugins-type-git": "cd plugins/type-git && npm run watch",
    "watch-plugins-determine-semver": "cd plugins/determine-semver && npm run watch",
    "watch-plugins-bump-package-json": "cd plugins/bump-package-json && npm run watch",
    "concurrently": "concurrently \"npm:watch-*\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "jest": "^26.0.1",
    "jest-junit": "^11.0.0",
    "md5": "^2.2.1",
    "typescript": "^3.9.5",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-jest": "^26.1.0"
  },
  "dependencies": {
    "concurrently": "^5.2.0"
  }
}
